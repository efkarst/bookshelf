<h1>Sign In</h1>
<%= form_tag('/sessions', method: 'post') do %>
  <div class="form-group">
    <%= label_tag "Email" %>
    <%= text_field_tag 'email' %>
  </div>

  <div class="form-group">
    <%= label_tag "Password" %>
    <%= password_field_tag 'password'%>
  </div>

  <div class="actions">
    <%= submit_tag 'Sign In' %>
  </div>

<% end %>
